.time-axis-wrap {
  height:  160px;
  padding: 30px;
  .right-time-range {
    float: left;
    margin:10px 0 0 30px;
    .time-key {
      text-align: center;
      color: #0e0e10;
      line-height: 24px;
      width: 90px;
      float: left;
      p {
        background: #1fffff;
        padding: 5px 20px;
        border-radius: 10px;
        max-height: 20px;
        float: left;
        cursor: pointer;
      } 
      .list-key {
        padding: 5px 20px;
        float: left;
        background: #1fffff;
        border-radius: 0 0 10px 10px;
        display: none;
        margin-top: -10px;
        span {
          display: block;
          cursor: pointer;
        }
      }
    }
  }
  .left-time-line {
    float: left;
    width: 64%;
    height: 90px;
    .timeline-dots {
      width: 100%;
      max-height: 90px;
      overflow-y: hidden;
      background: url(images/timeline-bg.png) left 15px repeat-x;
      li {
        float: left;
        margin: 30px;
        transition: all 1s cubic-bezier(0.455, 0.03, 0.515, 0.955);
        i {
          background: url(./images/top-line.png) no-repeat;
          display: block;
          margin: -24px 0 0 23px;
          width: 6px;
          height: 40px;
          top: 60px;
        }
        button {
          background: #18445e;
          margin-top: 10px;
          cursor: pointer;
          border: none;
          outline: none;
          color: #fff;
          font-size: 12px;
          text-align: center;
          border-radius: 2px;
          line-height: 26px;
          padding: 0 15px;
          
        }
      }
    }
  }
}
 