import zrender from 'zrender'

/**
 * 随机生成svg defs中元素的ID
 * 
 * @return 随机生成的ID
 */
export const genSVGDocID = ( () => {
  let id = 1
  return () => {
    let prefix = new Date().valueOf()
    return `hyfe-${prefix}-${id++}`
  }
})()

export const zrenderInit = (selector, width, height) => {
  const zr = zrender.init(document.querySelector(selector), {
    renderer: 'canavs',
    width: width,
    height: height
  })
  return zr 
}

